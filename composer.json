{
    "name": "pforret/digestif",
    "description": "This is my package digestif",
    "keywords": [
        "pforret",
        "digestif"
    ],
    "homepage": "https://github.com/pforret/digestif",
    "license": "MIT",
    "authors": [
        {
            "name": "Peter Forret",
            "email": "peter@forret.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "laravel/pint": "^1.2"
    },
    "autoload": {
        "psr-4": {
            "Pforret\\Digestif\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Pforret\\Digestif\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit --testdox",
            "test-coverage": "vendor/bin/phpunit --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {

            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "version": "1.0.2"
}
